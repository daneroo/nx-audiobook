{
  "name": "nx-audiobook",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "coverage": "vitest run --coverage && open coverage/index.html",
    "build": "nx run-many --target=build",
    "start": "node apps/validate/index.js",
    "format:write": "prettier --write \"packages/**/*.ts\" \"apps/**/*.ts\"",
    "affected:lint": "pnpm exec nx affected --target=lint --parallel=3",
    "affected:test": "pnpm exec nx affected --target=test --parallel=3",
    "affected:build": "pnpm exec nx affected --target=build --parallel=3"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.4.2",
    "@nrwl/nx-cloud": "15.2.1",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.29.2",
    "chalk": "^5.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "npm-check-updates": "^16.7.12",
    "nx": "15.8.6",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite-node": "^0.29.2",
    "vitest": "^0.29.2"
  },
  "packageManager": "pnpm@9.0.5+sha512.a722575c18fd791f9ef71e387c6e16cc03c90d859b9b0998d0428ca0e3220828b32a96e6de73cea34edbc0f50824771b1a69c7ea36b86daa3b89c7e6d9a3d912"
}
