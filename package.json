{
  "name": "nx-audiobook",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "coverage": "vitest run --coverage && open coverage/index.html",
    "build": "nx run-many --target=build",
    "start": "node apps/validate/index.js",
    "format:write": "prettier --write \"packages/**/*.ts\" \"apps/**/*.ts\"",
    "affected:lint": "pnpm exec nx affected --target=lint --parallel=3",
    "affected:test": "pnpm exec nx affected --target=test --parallel=3",
    "affected:build": "pnpm exec nx affected --target=build --parallel=3"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.3.1",
    "@nrwl/nx-cloud": "15.0.0",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.24.3",
    "chalk": "^5.1.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-check-updates": "^16.7.12",
    "nx": "15.0.4",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vite-node": "^0.24.3",
    "vitest": "^0.24.3"
  }
}